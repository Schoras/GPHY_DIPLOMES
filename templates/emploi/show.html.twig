{% extends 'base.html.twig' %}

{% block title %}Emploi{% endblock %}

{% block body %}
    <h1>Emploi</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdContrat</th>
                <td>{{ emploi.idContrat }}</td>
            </tr>
            <tr>
                <th>IdEntreprise</th>
                <td>{{ emploi.idEntreprise }}</td>
            </tr>
            <tr>
                <th>NumEtud</th>
                <td>{{ emploi.numEtud }}</td>
            </tr>
            <tr>
                <th>IdO</th>
                <td>{{ emploi.idO }}</td>
            </tr>
            <tr>
                <th>DebutC</th>
                <td>{{ emploi.debutC ? emploi.debutC|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>FinC</th>
                <td>{{ emploi.finC ? emploi.finC|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>TypeContrat</th>
                <td>{{ emploi.typeContrat }}</td>
            </tr>
            <tr>
                <th>DateMj</th>
                <td>{{ emploi.dateMj ? emploi.dateMj|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('emploi_index') }}">back to list</a>

    <a href="{{ path('emploi_edit', {'idContrat': emploi.idContrat}) }}">edit</a>

    {{ include('emploi/_delete_form.html.twig') }}
{% endblock %}
